layer {
  name: "data"
  type: "Data"
  top: "data"
  include {    phase: TRAIN  }
  transform_param {
   #mirror: true
   #crop_size: 176
   #mean_value:127
   #mean_value:127
   #mean_value:127
  }
  data_param {
    source: "E:\\DeepLearning\\myColor\\train_data"
    batch_size: 4
    backend: LMDB
  }
}
layer {
  name: "data"
  type: "Data"
  top: "data"
  include {    phase: TEST  }
  transform_param {
   #mirror: false
   #crop_size: 256
  }
  data_param {
    source: "E:\\DeepLearning\\myColor\\val_data"
    batch_size: 2
    backend: LMDB
  }
}
layer {
  name: "slice"
  type: "Slice"
  bottom: "data"
  top: "data_y" # 
  top: "label_uv" # 
  propagate_down: false
  slice_param {
    axis: 1
    slice_point: 1
  }
}
layer {
  name: "pool1"   
  type: "Pooling"  
  bottom: "label_uv"  
  top: "label_uv2"   

  pooling_param {
    pool: MAX
    kernel_size: 1  
    stride: 2  
  }
}
layer { 
  name: "data_meansub"
  type: "Scale"
  bottom: "data_y"
  top: "data_y2" 
  propagate_down: false
  param {lr_mult: 0 decay_mult: 0}
  param {lr_mult: 0 decay_mult: 0}
  scale_param {
    bias_term: True
    filler {      type: 'constant'      value: 1    }
    bias_filler {      type: 'constant'      value: -127    }
  }
}
# *****************
# ***** conv1 *****
# *****************
layer {
  name: "bw_conv1_1"
  type: "Convolution"
  bottom: "data_y2"
  top: "conv1_1"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  weight_filler {
      type: "xavier"
   }
   bias_filler {
      type: "constant"
   }
  }
}
layer {
  name: "relu1_1"
  type: "ReLU"
  bottom: "conv1_1"
  top: "conv1_1"
}
layer {
  name: "conv1_2"
